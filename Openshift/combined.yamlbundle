### FILE: splunk-integration/charts/openshift-logforwarding-splunk/templates/log-forwarding-splunk-statefulset.yaml ###
{{- $fullName := include "openshift-logforwarding-splunk.fullname" . -}}
{{- $splunkSecret := printf "%s-splunk" $fullName -}}
{{- $logForwardingConfig := printf "%s-config" $fullName -}}
{{- $logForwardingCertificateName := include "openshift-logforwarding-splunk.certificateName" . -}}
apiVersion: apps/v1
kind: StatefulSet
metadata:
  name: {{ $fullName }}
  namespace: {{ .Release.Namespace }}
  labels:
    app.kubernetes.io/component: fluentd
{{ include "openshift-logforwarding-splunk.labels" . | indent 4 }}
spec:
  replicas: {{ .Values.forwarding.fluentd.replicas }}
  serviceName: {{ $fullName }}-headless
  updateStrategy:
{{ toYaml .Values.forwarding.fluentd.updateStrategy | indent 4 }}
  selector:
    matchLabels:
      app: {{ $fullName }}
  template:
    metadata:
      labels:
        app: {{ $fullName }}
    spec:
      restartPolicy: Always
      terminationGracePeriodSeconds: 30
      containers:
        - resources:
            {{- toYaml .Values.forwarding.fluentd.resources | nindent 12 }}
          terminationMessagePath: /dev/termination-log
          name: {{ $fullName }}
          env:
            - name: NODE_NAME
              valueFrom:
                fieldRef:
                  apiVersion: v1
                  fieldPath: spec.nodeName
            - name: LOG_LEVEL
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: fluentd-loglevel
            - name: SHARED_KEY
              valueFrom:
                secretKeyRef:
                  name: {{ $fullName }}
                  key: shared_key
            - name: SPLUNK_HOST
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-hostname
            - name: SPLUNK_SOURCETYPE
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-sourcetype
            - name: SPLUNK_SOURCE
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-source
            - name: SPLUNK_PORT
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-port
            - name: SPLUNK_PROTOCOL
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-protocol
            - name: SPLUNK_INSECURE
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-insecure
            - name: SPLUNK_TOKEN
              valueFrom:
                secretKeyRef:
                  name: {{ $fullName }}
                  key: hec-token
            - name: SPLUNK_INDEX
              valueFrom:
                configMapKeyRef:
                  name: {{ $fullName }}
                  key: splunk-index
            {{- if .Values.forwarding.fluentd.ssl }}
            - name: FLUENTD_SSL_PASSPHRASE
              valueFrom:
                secretKeyRef:
                  name: {{ $fullName }}
                  key: fluentd-ssl-passsphrase
            {{- end }}
          imagePullPolicy: Always
          volumeMounts:
            - name: {{ $logForwardingConfig }}
              readOnly: true
              mountPath: /etc/fluent/
            {{- if .Values.forwarding.fluentd.ssl }}
            - mountPath: /secret/fluentd
              name: {{ $fullName }}
              readOnly: true
            {{- end }}
            - name: buffer
              mountPath: "/var/log/fluentd"
            {{- if and (not .Values.forwarding.splunk.insecure) .Values.forwarding.splunk.caFile }}
            - mountPath: /secret/splunk
              name: splunk-certs
              readOnly: true
            {{- end }}
          terminationMessagePolicy: File
          image: {{ .Values.forwarding.fluentd.image }}
          args:
{{- if .Values.forwarding.fluentd.scl }}
            - scl
            - enable
            - rh-ruby25
            - "--"
{{- end }}
            - fluentd
          livenessProbe:
            tcpSocket:
              port: 24224
            periodSeconds: 5
            timeoutSeconds: 3
            initialDelaySeconds: 10
          readinessProbe:
            tcpSocket:
              port: 24224
            periodSeconds: 3
            timeoutSeconds: 2
            initialDelaySeconds: 2
          ports:
            - containerPort: 24224
              name: forwarder-tcp
              protocol: TCP
            - containerPort: 24224
              name: forwarder-udp
              protocol: UDP
      serviceAccount: {{ $fullName }}
      {{- with .Values.forwarding.fluentd.nodeSelector }}
      nodeSelector:
        {{- toYaml . | nindent 8 }}
      {{- end }}
    {{- with .Values.forwarding.fluentd.affinity }}
      affinity:
        {{- toYaml . | nindent 8 }}
    {{- end }}
    {{- with .Values.forwarding.fluentd.tolerations }}
      tolerations:
        {{- toYaml . | nindent 8 }}
    {{- end }}
      volumes:
        - name: {{ $logForwardingConfig }}
          configMap:
            name: {{ $fullName }}
            items:
              - key: td-agent.conf
                path: fluent.conf
            defaultMode: 420
            optional: true
        {{- if .Values.forwarding.fluentd.ssl }}
        - name: {{ $fullName }}
          secret:
            secretName: {{ $fullName }}-certs
            items:
              - key: tls.crt
                path: tls.crt
              - key: tls.key
                path: tls.key
        {{- end }}
        {{- if not .Values.forwarding.fluentd.persistence.enabled }}
        - name: buffer
          emptyDir: {}
        {{- end }}
        {{- if and (not .Values.forwarding.splunk.insecure) .Values.forwarding.splunk.caFile }}
        - name: splunk-certs
          secret:
            secretName: {{ $fullName }}-splunk
            items:
              - key: splunk-ca.crt
                path: splunk-ca.crt
        {{- end }}
      dnsPolicy: ClusterFirst
  {{- if .Values.forwarding.fluentd.persistence.enabled }}
  volumeClaimTemplates:
  - metadata:
      name: buffer
    spec:
      accessModes:
      - {{ .Values.forwarding.fluentd.persistence.accessMode | quote }}
      resources:
        requests:
          storage: {{ .Values.forwarding.fluentd.persistence.size }}
      {{- if .Values.forwarding.fluentd.persistence.storageClass }}
      {{- if (eq "-" .Values.forwarding.fluentd.persistence.storageClass) }}
      storageClassName: ""
      {{- else }}
      storageClassName: "{{ .Values.forwarding.fluentd.persistence.storageClass }}"
      {{- end }}
      {{- end }}
  {{- end }}


### FILE: splunk-integration/charts/openshift-logforwarding-splunk/values.yaml ###
openshift:
  logging:
    namespace: openshift-logging
    elasticsearch:
      port: 9200
  forwarding:
    audit:
      elasticsearch: true
      splunk: true
    app:
      elasticsearch: true
      splunk: false
    infra:
      elasticsearch: true
      splunk: false
  # kubeVersion: v1.20

forwarding:
  fluentd:
    port: 24224
    sharedkey: splunkforwarding
    passphrase: ""
    ssl: true
    caFile: files/default-openshift-logging-fluentd.crt
    keyFile: files/default-openshift-logging-fluentd.key
    loglevel: warn
    replicas: 2
    # Set to true when version <4.6
    scl: false
    persistence:
      enabled: false
      size: 5Gi
      ## If defined, storageClassName: <storageClass>
      ## If set to "-", storageClassName: "", which disables dynamic provisioning
      ## If undefined (the default) or set to null, no storageClassName spec is
      ##   set, choosing the default provisioner.  (gp2 on AWS, standard on
      ##   GKE, AWS & OpenStack)
      ##
      # storageClass: "-"
      storageClass: ""
      accessMode: ReadWriteOnce
    image: registry.redhat.io/openshift4/ose-logging-fluentd:v4.6
    nodeSelector: {}
    tolerations: []
    affinity: {}
    resources:
      requests:
        cpu: 100m
        memory: 512Mi
      limits:
        cpu: 500m
        memory: 1024Mi
    updateStrategy:
      type: "RollingUpdate"
    buffer:
      "@type": memory
      chunk_limit_records: 100000
      chunk_limit_size: 200m
      flush_interval: 5s
      flush_thread_count: 1
      overflow_action: block
      retry_max_times: 3
      total_limit_size: 600m
  # Example configuration to support file based buffering
      # "@type": file
      # path: /var/log/fluentd/fluentd-buffers/buffer
      # flush_mode: interval
      # retry_type: exponential_backoff
      # flush_thread_count: 2
      # flush_interval: "5s"
      # retry_forever:
      # retry_max_interval: 30
      # chunk_limit_size: "200m"
      # total_limit_size: "600m"
      # chunk_limit_records: 100000
      # overflow_action: block
  splunk:
    # Specify Splunk HEC Token and Index
    token:
    index: openshiftsandbox
    protocol: https
    hostname: http-inputs-qubit.splunkcloud.com
    port: 443
    insecure: false
    sourcetype: openshift
    source: openshift

    # Specify the CA Certificate for Splunk
    caFile: "files/splunk-ca.crt"


