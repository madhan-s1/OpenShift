                              type: integer
                            protocol:
                              type: string
                          type: object
                        type: array
                      endpoints:
                        type: string
                      lastRequest:
                        format: date-time
                        type: string
                      tenantUUID:
                        type: string
                    type: object
                  imageID:
                    type: string
                  instances:
