### FILE: Gitops/manifests/cluster-config/ldapgroupsync/base/ldap-sync-config-map.yaml ###
kind: ConfigMap
apiVersion: v1
metadata:
  annotations:
    argocd.argoproj.io/sync-wave: "2"
  name: ldap-group-syncer
  namespace: ldap-sync
data:
  sync.yaml: |
    kind: LDAPSyncConfig
    apiVersion: v1
    url: ldaps://intldsp.entlds.apptoapp.net:636
    insecure: false
    bindDN: "CN=S700232,OU=Users,DC=IntLDSP,DC=EntLDS,DC=apptoapp,DC=net"
    bindPassword:
      file: "/etc/secrets/bindPassword"
    ca: /etc/ldap-ca/ca.crt
    rfc2307:
      groupsQuery:
        baseDN: "DC=IntLDSP,DC=EntLDS,DC=apptoapp,DC=net"
        scope: sub
        filter: (CN=G-OCP*)
        derefAliases: never
      groupUIDAttribute: dn
      groupNameAttributes: [ cn ]
      groupMembershipAttributes: [ member ]
      usersQuery:
        baseDN: "DC=IntLDSP,DC=EntLDS,DC=apptoapp,DC=net"
        scope: sub
        derefAliases: never
      userUIDAttribute: dn
      userNameAttributes: [ cn ]
      tolerateMemberNotFoundErrors: false
      tolerateMemberOutOfScopeErrors: false


### FILE: Gitops/manifests/cluster-config/ldapgroupsync/base/ldap-sync-cron-job.yaml ###
kind: CronJob
apiVersion: batch/v1
metadata:
  annotations:
    argocd.argoproj.io/sync-wave: "4"
  name: ldap-group-syncer
  namespace: ldap-sync
spec:                                                                                
  schedule: "59 1 * * *"                                                           
  concurrencyPolicy: Forbid
  jobTemplate:
    spec:
      backoffLimit: 0
      template:
        spec:
          containers:
            - name: ldap-group-sync
              image: "registry.redhat.io/openshift4/ose-cli:latest"
              command:
                - "/bin/bash"
                - "-c"
                - "oc adm groups sync --sync-config=/etc/config/sync.yaml --confirm ; oc adm prune groups --sync-config=/etc/config/sync.yaml --confirm" 
              volumeMounts:
                - mountPath: "/etc/config"
                  name: "ldap-sync-volume"
                - mountPath: "/etc/secrets"
                  name: "ldap-bind-password"
                - mountPath: "/etc/ldap-ca"
                  name: "ldap-ca"
          nodeSelector:
            node-role.kubernetes.io/infra: ""
          tolerations:
          - effect: NoSchedule
            key: node-role.kubernetes.io/infra
            operator: Exists
          volumes:
            - name: "ldap-sync-volume"
              configMap:
                name: "ldap-group-syncer"
            - name: "ldap-bind-password"
              secret:
                secretName: "ldap-secret"                                            
            - name: "ldap-ca"
              configMap:
                name: "ca-config-map"                                                
          restartPolicy: "Never"
          terminationGracePeriodSeconds: 30
          activeDeadlineSeconds: 500
          dnsPolicy: "ClusterFirst"
          serviceAccountName: "ldap-group-syncer"


