    api_version: argoproj.io/v1alpha1
    kind: Application
    name: config-etcdbackup
    namespace: openshift-gitops
    validate_certs: no
  register: ocp_gitops_application_status
  until: ocp_gitops_application_status.resources[0].status.health.status == "Healthy"
