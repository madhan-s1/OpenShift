- name: ocp_gitops | ocp_gitops_cluster-config_apps_validation_worker-guardium-machineset.yaml | Openshift Gitops - Get cluster infrastructure id
  ansible.builtin.shell: |
    oc get -o jsonpath='{.status.infrastructureName}{"\n"}' infrastructure cluster
  register: ocp_gitops_infrastructure_id

- block:
    - name: ocp_gitops | ocp_gitops_cluster-config_apps_validation_worker-guardium-machineset.yaml | Openshift Gitops - Validate worker-guardium machineset
      kubernetes.core.k8s_info:
        host: "{{ ocp_gitops_openshift_host }}"
        validate_certs: no
        api_version: machine.openshift.io/v1beta1
        kind: MachineSet
        name: "{{ ocp_gitops_infrastructure_id.stdout }}-worker-guardium"
        namespace: openshift-machine-api
      register: ocp_gitops_worker-guardium_machineset
      until:
        - ocp_gitops_worker-guardium_machineset.resources[0].status.readyReplicas is defined
        - ocp_gitops_worker-guardium_machineset.resources[0].status.readyReplicas == ocp_gitops_worker-guardium_machineset.resources[0].status.replicas
      retries: 5 
      delay: 30

  rescue:

    - name: ocp_gitops | ocp_gitops_cluster-config_apps_validation_worker-guardium-machineset.yaml | Openshift Gitops - Validate worker-guardium machineset status
      when: 
        - ocp_gitops_worker-guardium_machineset.resources[0].status.readyReplicas is not defined
      debug:
        msg: "worker-guardium machineset not ready"

    - name: ocp_gitops | ocp_gitops_cluster-config_apps_validation_worker-guardium-machineset.yaml | Openshift Gitops - Validate worker-guardium machineset status
